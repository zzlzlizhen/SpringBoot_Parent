<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.UserMapper">

    <sql id="Base_Column_List" >
   		id,name,pwd,phone,email,create_time,update_time
    </sql>
	<insert id="save">
	    insert into t_user(id,name,pwd,phone,email,create_time,update_time) values(#{user.id},#{user.name},#{user.pwd},#{user.phone},#{user.email},#{user.createTime},#{user.updateTime})
	</insert>
</mapper>
